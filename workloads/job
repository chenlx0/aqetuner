SELECT kind_id, production_year, title, COUNT(*)  FROM title t,movie_info mi,movie_info_idx mi_idx,cast_info ci WHERE t.id=mi.movie_id AND t.id=mi_idx.movie_id AND t.id=ci.movie_id AND mi.info_type_id=3 AND mi_idx.info_type_id=100 GROUP BY kind_id, production_year, title;
SELECT kind_id, imdb_id, COUNT(*)  FROM title t,movie_keyword mk,movie_companies mc,movie_info mi WHERE t.id=mk.movie_id AND t.id=mc.movie_id AND t.id=mi.movie_id AND mk.keyword_id=398 AND mc.company_type_id=2 AND t.production_year>2000 AND t.production_year<2010 GROUP BY kind_id, imdb_id;
SELECT kind_id, title, COUNT(*)  FROM title t,movie_info mi,movie_info_idx mi_idx,movie_keyword mk,movie_companies mc WHERE t.id=mi.movie_id AND t.id=mk.movie_id AND t.id=mi_idx.movie_id AND t.id=mc.movie_id AND t.production_year>2009 AND mi.info_type_id=8 AND mi_idx.info_type_id=101 GROUP BY kind_id, title;
SELECT person_id, COUNT(*)  FROM cast_info ci,title t WHERE t.id=ci.movie_id AND t.production_year>1980 AND t.production_year<1995 GROUP BY person_id;
SELECT imdb_id, title, COUNT(*)  FROM title t,movie_info mi,movie_keyword mk WHERE t.id=mi.movie_id AND t.id=mk.movie_id AND t.production_year>2005 GROUP BY imdb_id, title;
SELECT imdb_id, COUNT(*)  FROM title t,movie_info mi,movie_info_idx mi_idx,cast_info ci WHERE t.id=mi.movie_id AND t.id=mi_idx.movie_id AND t.id=ci.movie_id AND mi.info_type_id=3 AND mi_idx.info_type_id=100 GROUP BY imdb_id;
SELECT person_role_id, COUNT(*)  FROM cast_info ci,title t,movie_companies mc WHERE t.id=ci.movie_id AND t.id=mc.movie_id AND ci.role_id=4 GROUP BY person_role_id;
SELECT movie_companies.movie_id, company_id, COUNT(*)  FROM movie_companies mc,title t,movie_keyword mk WHERE t.id=mc.movie_id AND t.id=mk.movie_id AND mk.keyword_id=117 GROUP BY movie_companies.movie_id, company_id;